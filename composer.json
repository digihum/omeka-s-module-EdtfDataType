            "EdtfDataTypeTest\\": "test/EdtfDataTypeTest/"
        }
    },
    "require-dev": {
        "biblibre/omeka-s-test-helper": "dev-master",
        "phpunit/phpunit": "9.5"
    },
    "extra": {
        "installer-name": "EdtfDataType"
    }
