{
    "name": "digihum/omeka-s-module-edtfdatatype",
    "description": "EDTF Data Type (module for Omeka S)",
    "type": "omeka-s-module",
    "license": "CECILL-2.1",
    "authors": [
        {
            "name": "Steve Ranford",
            "email": "digitalhumanities@warwick.ac.uk"
        }
    ],
    "homepage": "https://warwick.ac.uk/digitalhumanities",
    "support": {
        "issues": "https://github.com/digihum/omeka-s-module-edtfdatetime",
        "forum": "https://forum.omeka.org/"
    },
    "keywords": [
        "Omeka S",
